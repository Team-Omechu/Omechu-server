  // 달걀(난류) 알레르기 @map("달걀(난류) 알레르기")
  // 우유 알레르기 @map("우유 알레르기")
  // 갑각류 알레르기 @map("갑각류 알레르기")
  // 해산물 알레르기 @map("해산물 알레르기")
  // 견과류 알레르기 @map("견과류 알레르기")
}

